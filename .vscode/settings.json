{
    "editor.tokenColorCustomizations": {        
        "comments": "#00960c"
    },
    "openInGitHub.remote.name": "/QuentinBultinck/RetroAchievements",
    "todo-tree.general.tags": [
        "$",

        "DONE",
        "WORKS",
        "WORKING",

        "TODO",

        "FIXME",
        "BUG",

        "WIP",
        "TEST",
        "TESTING"
    ],
    "todo-tree.general.tagGroups": {
        "DONE": [
            "DONE",
            "WORKING",
            "WORKS"
        ],
        "FIXME": [
            "FIXME",
            "BUG",
        ],
        "TEST": [
            "TEST",
            "TESTING"
        ]
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "#1cdb07",
        "background": "#164f04",
        "type": "tag-and-comment", 
        "opacity": 42,              
        "icon": "info",
        "gutterIcon": true
    },
    "todo-tree.highlights.customHighlight": {
        "$": {
            "foreground": "#02a80a",
            "background": "#6a7c05",
            "opacity": 12,
            "icon": "bookmark",
            "type": "text-and-comment",
            "rulerLane": "right",
            "hideFromStatusBar": true,
            "hideFromActivityBar": true
        },
        "TODO": {
            "icon": "issue-draft" 
        },
        "WIP": {
            "foreground": "#ffe54f",
            "background": "#027c16",            
            "type": "whole-line", 
            "opacity": 37,
            "icon": "tools"
        },
        "TEST": {
            "foreground": "#fad55c",
            "background": "#037a23",
            "type": "line", 
            "opacity": 37,  
            "icon": "unverified"
        },
        "DONE": {   
            "icon": "verified"            
        },
        "FIXME": {
            "foreground": "#fc5353",
            "background": "#612c03",
            "type": "text-and-comment", 
            "opacity" : 42,
            "icon": "stop"
        }
    },
    "todo-tree.highlights.enabled": true,
    "todo-tree.regex.regex": "((//|#|<!--|;|/\\*|^)\\s*($TAGS)|^//\\s*\\[x\\]|^//\\s*\\[ \\])",
    "todo-tree.filtering.includedWorkspaces": [
        "RAscripts"
    ], 
    "todo-tree.filtering.excludeGlobs": [
        "RAscripts\/PSP\/ShadowOfDestiny\/Region_USA\/SoD_Generated.rascript"
    ],
    "todo-tree.general.showIconsInsteadOfTagsInStatusBar": true,
    "todo-tree.general.statusBar": "current file",
    "todo-tree.general.showActivityBarBadge": true
}